<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.mycan.app_mycan" >

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:logo="@drawable/ic_launcher"
        android:label="@string/app_name"
        android:theme="@style/AppTheme">
        <activity
            android:name=".MainActivity"
            android:label="@string/app_name"
            android:screenOrientation="portrait" >
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity
            android:name=".ListActivity"
            android:logo="@drawable/ic_launcher"
            android:label="@string/title_activity_list"
            android:screenOrientation="portrait" >
            <intent-filter>
                <category android:name="android.intent.category.HOME" />
            </intent-filter>
        </activity>
        <activity
            android:name=".InsertarMascota"
            android:logo="@drawable/ic_launcher"
            android:label="@string/title_activity_insertar_mascota"
            android:screenOrientation="portrait" >
        </activity>
        <activity
            android:name=".InsertarCita"
            android:logo="@drawable/ic_launcher"
            android:label="@string/title_activity_insertar_cita"
            android:screenOrientation="portrait" >
        </activity>
        <activity
            android:name=".ListCitas"
            android:logo="@drawable/ic_launcher"
            android:label="@string/title_activity_list_citas"
            android:screenOrientation="portrait">
        </activity>
    </application>

</manifest>
